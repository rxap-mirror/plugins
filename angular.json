{
  "version": 1,
  "projects": {
    "plugin-library-schematics": {
      "root": "libs/library-schematics",
      "sourceRoot": "libs/library-schematics/src",
      "projectType": "library",
      "schematics": {},
      "prefix": "rxap-plugin",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/library-schematics/tsconfig.lib.json",
              "libs/library-schematics/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/library-schematics/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/library-schematics/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/library-schematics"
          ]
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/library-schematics",
            "tsConfig": "libs/library-schematics/tsconfig.lib.json",
            "packageJson": "libs/library-schematics/package.json",
            "main": "libs/library-schematics/src/index.ts",
            "assets": [
              "libs/library-schematics/*.md",
              {
                "input": "./libs/library-schematics/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./libs/library-schematics",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./libs/library-schematics",
                "glob": "builders.json",
                "output": "."
              }
            ],
            "srcRootForCompilationRoot": "libs/library-schematics"
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "pack": {
          "builder": "@rxap/plugin-pack:build",
          "options": {
            "targets": [
              "plugin-library-schematics:readme",
              "plugin-library-schematics:build"
            ]
          }
        },
        "readme": {
          "builder": "@rxap/plugin-readme-generator:plugin"
        }
      }
    },
