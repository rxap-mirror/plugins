{
  "version": 2,
  "outputCapture": "direct-nodejs",
  "$schema": "http://json-schema.org/schema",
  "$id": "firebase-hosting",
  "title": "Hosting",
  "type": "object",
  "properties": {
    "channel": {
      "type": "string",
      "default": "live"
    },
    "project": {
      "type": "string"
    },
    "token": {
      "type": "string"
    },
    "target": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "expires": {
      "type": "string"
    }
  },
  "required": [
    "target"
  ]
}
