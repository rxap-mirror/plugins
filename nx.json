{
  "npmScope": "plugins",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {},
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "pack", "test", "lint", "e2e"],
        "parallel": 1
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "pack": [
      {
        "target": "pack",
        "projects": "dependencies"
      }
    ]
  },
  "cli": {
    "defaultCollection": "@rxap/schematics",
    "packageManager": "yarn"
  }
}
